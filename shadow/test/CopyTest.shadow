		for( int i = 0; i < 2; i += 1 )
			for( int j = 0; j < array9[i]->size; j += 1 )
				array9[i][j] = 3 * i + j;
				
		int[][] array10 = copy(array9);
		for( int i = 0; i < 2; i += 1 )
			for( int j = 0; j < array9[i]->size; j += 1 )
				array9[i][j] *= 2;
				
		out.printLine("array9: " # array9 );		
		out.printLine("array10: " # array10 );		
		
