class Casting {
    // casting for members
     int a = cast<int>(4);
     int d = cast<int>(a);


	//check all types
     float f = cast<float>(4);
     double g = cast<double>(4);
     code h = cast<code>(4);
     byte i = cast<byte>(4);
     boolean j = cast<boolean>(false);
     Object o = Object:create();     
     String k = cast<String>(o);  
    
    //() => () fun = cast<()=>()>(blah);        
    
    public blah() => () {
	    // now in a method
	    int a = cast<int>(4);	    
	    int d = cast<int>(a);
	
		//check all types
	    float f = cast<float>(4);
	    double g = cast<double>(4);
	    code h = cast<code>(4);
	    byte i = cast<byte>(4);
	    boolean j = cast<boolean>(false);
	    Object o = Object:create();     
	    String k = cast<String>(o);  
	    
	    //() => () fun = cast<()=>()>(blah);        
	    
	    //crazy cast	        
        //(int[][][], int, int ) => ( ()=>(int[][][])) b  = cast<(int[][][], int, int ) => ( ()=>(int[][][]))>(blah);        
   }
}
