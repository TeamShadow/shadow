{ debugPrint("BLOCK"); Token t; }
{
  "{" ( BlockStatement() )* t = "}"
  {
    jjtThis.setEndToken(t);  }
}

void BlockStatement():
{
    debugPrint("BLOCK STATEMENT");    
}
{
  LOOKAHEAD(LocalDeclaration()) LocalDeclaration()    
| Statement()
// No local classes in Shadow
}

