void Statement():
{ debugPrint("STATEMENT"); }
{  
  AssertStatement()
| Block()
| EmptyStatement()
| StatementExpression() ";"
| SwitchStatement()
| IfStatement()
| WhileStatement()
| DoStatement()
| ForStatement()
| ForeachStatement()
| BreakOrContinueStatement()
| ReturnStatement()
| ThrowStatement()
| FinallyStatement() //includes try/catch/recover/finally
}

