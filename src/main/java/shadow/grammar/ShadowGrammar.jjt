void AssignmentOperator():
{ debugPrint("ASSIGNMENT OPERATOR"); }
{
    "="    { jjtThis.setImage("="); jjtThis.setAssignmentType(ASTAssignmentOperator.AssignmentType.EQUAL); }
  | "#="   { jjtThis.setImage("#="); jjtThis.setAssignmentType(ASTAssignmentOperator.AssignmentType.CAT); }
  | "*="   { jjtThis.setImage("*="); jjtThis.setAssignmentType(ASTAssignmentOperator.AssignmentType.STAR); }
  | "/="   { jjtThis.setImage("/="); jjtThis.setAssignmentType(ASTAssignmentOperator.AssignmentType.SLASH); }
  | "%="   { jjtThis.setImage("%="); jjtThis.setAssignmentType(ASTAssignmentOperator.AssignmentType.MOD); }
  | "+="   { jjtThis.setImage("+="); jjtThis.setAssignmentType(ASTAssignmentOperator.AssignmentType.PLUS); }
  | "-="   { jjtThis.setImage("-="); jjtThis.setAssignmentType(ASTAssignmentOperator.AssignmentType.MINUS); }
  | "<<="  { jjtThis.setImage("l="); jjtThis.setAssignmentType(ASTAssignmentOperator.AssignmentType.LEFT_SHIFT); }
  | ">>="  { jjtThis.setImage("r="); jjtThis.setAssignmentType(ASTAssignmentOperator.AssignmentType.RIGHT_SHIFT); }
  | ">>>=" { jjtThis.setImage("L="); jjtThis.setAssignmentType(ASTAssignmentOperator.AssignmentType.RIGHT_ROTATE); }
  | "<<<=" { jjtThis.setImage("R="); jjtThis.setAssignmentType(ASTAssignmentOperator.AssignmentType.LEFT_ROTATE); }
  | "&="   { jjtThis.setImage("&="); jjtThis.setAssignmentType(ASTAssignmentOperator.AssignmentType.AND); }
  | "^="   { jjtThis.setImage("^="); jjtThis.setAssignmentType(ASTAssignmentOperator.AssignmentType.XOR); }
  | "|="   { jjtThis.setImage("|="); jjtThis.setAssignmentType(ASTAssignmentOperator.AssignmentType.OR); }
}

void ConditionalExpression():
{}
{
  CoalesceExpression() [ "?" ConditionalExpression() "," ConditionalExpression() ]
}

void CoalesceExpression():
{}
{
  ConditionalOrExpression()  ( "??" ConditionalOrExpression() )*
}

void ConditionalOrExpression():
{}
{
  ConditionalExclusiveOrExpression() ( "or" { jjtThis.addToImage('o'); } ConditionalExclusiveOrExpression() )*
}


void ConditionalExclusiveOrExpression():
{}
{
  ConditionalAndExpression() ( "xor" { jjtThis.addToImage('x'); } ConditionalAndExpression() )*
}

void ConditionalAndExpression():
{}
{
  BitwiseOrExpression() ( "and" { jjtThis.addToImage('a'); } BitwiseOrExpression() )*
}

void BitwiseOrExpression():
{}
{
  BitwiseExclusiveOrExpression() ( "|" { jjtThis.addToImage('|'); } BitwiseExclusiveOrExpression() )*
}

void BitwiseExclusiveOrExpression():
{}
{
  BitwiseAndExpression() ( "^" { jjtThis.addToImage('^'); } BitwiseAndExpression() )*
}

void BitwiseAndExpression():
{}
{
  EqualityExpression() ( "&" { jjtThis.addToImage('&'); } EqualityExpression() )*
}

void EqualityExpression():
{}
{
  IsExpression() ( ( "==" { jjtThis.addToImage('='); } |
  					 "===" { jjtThis.addToImage('e'); } |
                     "!=" { jjtThis.addToImage('!'); } |
                     "!==" { jjtThis.addToImage('n'); } ) IsExpression() )*
}

void IsExpression():
{}
{
  RelationalExpression() [ "is" { jjtThis.addToImage('i'); } Type(Modifiers.NO_MODIFIERS) ]
}

void RelationalExpression():
{ debugPrint("RELATIONAL EXPRESSION"); }
{
  ConcatenationExpression() ( ( "<" { jjtThis.addToImage('<'); } |
                        ">" { jjtThis.addToImage('>'); } |
                        "<=" { jjtThis.addToImage('{'); } |
                        ">="  { jjtThis.addToImage('}'); }) ConcatenationExpression() )*
}


void ConcatenationExpression():
{ debugPrint("CONCATENATION EXPRESSION"); }
{
  ShiftExpression() ( "#" { jjtThis.addToImage('#'); }  ShiftExpression() )*
}


// Since shift & rotate are not single characters (see TAC Symbole2Operator code)
// we are using r & l for right & left shift and
// R & L for right & left rotate
void ShiftExpression():
{ debugPrint("SHIFT EXPRESSION"); }
{
  RotateExpression()
  ( ( "<<" { jjtThis.addToImage('l'); } |
      LOOKAHEAD({ getToken(1).kind == GT && ((GTToken)getToken(1)).realKind == RIGHTSHIFT} ) ">" ">" { jjtThis.addToImage('r'); } ) // >> needs special treatment
      RotateExpression() )*
}

void RotateExpression():
{ debugPrint("ROTATE EXPRESSION"); }
{
  AdditiveExpression()
  ( ( "<<<" { jjtThis.addToImage('L'); } |
      LOOKAHEAD({ getToken(1).kind == GT && ((GTToken)getToken(1)).realKind == RIGHTROTATE} ) ">" ">" ">"  { jjtThis.addToImage('R'); } ) // >>> needs special treatment
      AdditiveExpression() )*
}


void AdditiveExpression():
{}
{
  MultiplicativeExpression()
  ( ( "+" { jjtThis.addToImage('+'); } |
      "-" { jjtThis.addToImage('-'); } ) MultiplicativeExpression() )*
}

void MultiplicativeExpression():
{}
{
  UnaryExpression()
  ( ( "*" { jjtThis.addToImage('*'); } |
      "/" { jjtThis.addToImage('/'); } |
      "%" { jjtThis.addToImage('%'); } ) UnaryExpression() )*
}

void UnaryExpression():
{}
{
  //removed unary + since it's useless   
 "#" { jjtThis.setImage("#"); } UnaryExpression()
| "~" { jjtThis.setImage("~"); } UnaryExpression()
| "!" { jjtThis.setImage("!"); } UnaryExpression()
//| LOOKAHEAD(PrimaryExpression()) PrimaryExpression() //covers cases in which literals start with -
| "-" { jjtThis.setImage("-"); } UnaryExpression()
| LOOKAHEAD(FormalParameters(true) "=>") InlineMethodDefinition()
| PrimaryExpression()
}

