        int a;
        int b = 1;
        int c = 2;

        // literals
        if(true) { a = 0; }
        if(true and false) { a = 0; }
        if(true or false) { a = 0; }
        
        // basic relations
        if(a == 0) { a = 0; }
        if(a < 0) { a = 0; }
        if(a > 0) { a = 0; }
        if(a <= 0) { a = 0; }
        if(a >= 0) { a = 0; }
        if(a != 0) { a = 0; }
        if((2 + 3) == (a + b)) { a = 0; }
        
        // simple ANDs
        if(a == 0 and b == 1) { a = 0; }
        if(a > 0 and b < 1) { a = 0; }
        if((2 + 3) == (a + b) and (a + b) > (1 + 2)) { a = 0; }

